{
  "name": "fc-calendar",
  "version": "2.0.1",
  "main": "index.js",
  "repository": {
    "url": "git@github.com:fusioncharts/fc-calendar.git",
    "type": "git"
  },
  "author": "FusionCharts, Inc.  <support@fusioncharts.com>",
